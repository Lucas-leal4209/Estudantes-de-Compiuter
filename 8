// Online C compiler to run C program online
#include <stdio.h>

struct Veiculo{
    char modelo[20];
    int ano;
    char placa[7];
    char proprietario[20];
};

int main() {
    struct Veiculo carros[20];
    int n;
    
    printf("Quantos carros gostaria de informar?\n");
    scanf("%d",&n);
    
    for (int i=0;i<n;i++){
        printf("Informe o modelo do carro:\n");
        scanf("%s",carros[i].modelo);
        
        printf("Informe o ano de fabricação do carro:\n");
        scanf("%d",&carros[i].ano);
        
        printf("Informe a placa do carro:\n");
        scanf("%s",carros[i].placa);
        
        printf("Informe o nome do proprietário:\n");
        scanf("%s",carros[i].proprietario);
        
        printf("Modelo:%s\n",carros[i].modelo);
        printf("Ano de Fabricação:%d\n",carros[i].ano);
        printf("Placa do Carro:%s\n",carros[i].placa);
        printf("Nome do Proprietário:%s\n",carros[i].proprietario);
        
    }
    

    return 0;
}
