#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void encherVetor(int* vetor, int tamanho){
    srand((time)NULL);
    for (int i=0;i<tamanho;i++){
        vetor[i] = rand()%50+1;
    }
}

int main() {
    int tamanho;
    printf("Informe o tamanho do vetor:\n");
    scanf("%d",&tamanho);
    
    int* vetor = (int*) malloc(tamanho * sizeof (int));
    encherVetor(vetor, tamanho);
    
    printf("Vetor:\n");
    for (int i=0;i<tamanho;i++){
        printf("%d ",vetor[i]);
    }
    free(vetor);

    return 0;
}
